let index = 0
let neopixel = light.createNeoPixelStrip()
let start = 0
let end = 0
let index5 = 0
loops.forever(() => {
    for (let index4 = start; index4 <= end; index4++) {
        neopixel.setPixelColor(index4, Colors.Red)
    }
start += 1
    end += 1
})
index5 = 0
end = start + 5
index = 0
neopixel = light.createNeoPixelStrip(
pins.A0,
60,
NeoPixelMode.RGB
)
for (let index3 = 0; index3 <= 60; index3++) {
    neopixel.setPixelColor(index3, Colors.White)
}
